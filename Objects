const courses = [
{id: 1, name: 'a'},
{id: 2, name: 'b'},
];

const course = courses.find(function(course) {
return course.name === 'a'
});

// arrow function ?? same as lamba expression ?
const course = courses.find(course => course.name === 'a');

console.log(course);
